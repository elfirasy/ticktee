<h1>Projects</h1>
<%= link_to "New Project", new_project_path %>

<h2>Projects List</h2>
<ul>
<% @project.each do |project| %>
<li>
  <%= link_to project.name, project %> <br/>
  <%= link_to "Edit", edit_project_path(project) %> |
  <%= link_to 'Delete', project, confirm: 'Are you sure?', method: :delete %>
</li>
<% end %>
</ul>
